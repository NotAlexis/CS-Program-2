#include <iostream> 
using namespace std;

int main ()

{
    cout << " @@@@@@@\n" << "@\t@\n" << "@  O O  @\n"
        << "@   |   @\n" << "@  ---  @\n" << " @@@@@@@\n";
   
    
    
    
    
}
